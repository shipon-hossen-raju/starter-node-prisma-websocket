                  "// Check if the response is successful\r",
                  "if (pm.response.json().success) {\r",
                  "    // Extract the role and access token from the response\r",
                  "    const role = pm.response.json().data?.userData?.role;\r",
                  "    const accessToken = pm.response.json().data?.token;\r",
                  " \r",
                  "    // Set the token based on the user's role\r",
                  "     if (role === 'USER') {\r",
                  "        pm.collectionVariables.set(\"userToken\", accessToken);\r",
                  "    } else if (role === 'ADMIN') {\r",
                  "        pm.collectionVariables.set(\"adminToken\", accessToken);\r",
                  "     }\r",
                  "} else {\r",
                  "    // If the response is not successful, set the token to null\r",
